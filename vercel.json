{
  "version": 2,
  "regions": ["sin1"],
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "regions": ["sin1"]
    }
  },
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
